{
  "name": "cta/wp-theme",
  "description": "Continuity of Care Services WordPress Theme",
  "type": "wordpress-theme",
  "license": "proprietary",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "CCS\\": "src/"
    },
    "files": [
      "inc/ajax-handlers.php",
      "inc/theme-setup.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "CCS\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "phpstan": "phpstan analyse src --level=5",
    "cs": "phpcs --standard=WordPress src",
    "cs-fix": "phpcbf --standard=WordPress src"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
